<odoo>
    <record id="ji_category" model="ir.module.category">
        <field name="name">Premisos Jibarito</field>
        <field name="description">
            Configuracion para ver campos y objetos
        </field>
        <field name="sequence">5</field>
    </record>
    <record id="group_ji_apply_developments" model="res.groups">
        <field name="name">Group for apply developments</field>
